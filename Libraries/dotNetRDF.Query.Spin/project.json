{
  "name": "dotNetRDF.Query.Spin",
  "title": "dotNetRDF SPIN",
  "version": "1.0.13-pre6",
  "description": "A library which provides a full SPIN implementation using dotNetRDF's Leviathan SPARQL engine",
  "copyright": "Copyright Â© dotNetRDF Project 2009-2016",

  "dependencies": {
    "dotNetRDF": "1.0.13-pre6"
  },

  "frameworks": {
    "net40": {
      "buildOptions": {
        "define": [ "TRACE", "NET40" ]
      }
    }
  },

  "buildOptions": {
    "keyFile": "../../Build/dotNetRDF.snk",
    "outputName": "dotNetRDF.Query.Spin",
    "compile": {
      "excludeFiles": [
        "Constraints/SPINConstraints.cs",
        "Core/ContainsVarChecker.cs",
        "Core/ObjectPropertiesGetter.cs",
        "Core/SPINUtil.cs",
        "Util/DatasetUtil.cs"
      ]
    },
    "xmlDoc": true
  }

}

