{
  "name": "dotNetRDF",
  "title": "dotNetRDF",
  "version": "1.0.13-pre6",
  "description": "dotNetRDF is a RDF, SPARQL and Semantic Web API for .Net.  It provides simple but powerfully extensible APIs for this and integrates with a variety of popular triple stores.",
  "copyright": "Copyright Â© dotNetRDF Project 2009-2017",

  "authors": [
    "RobVesse",
    "tpluscode",
    "kal_ahmed",
    "ronmichael"
  ],
  "packOptions": {
    "tags": [ "RDF", "Semantic", "Web", "SPARQL", "RDF/XML", "Turtle", "Notation3" ],
    "iconUrl": "http://www.w3.org/RDF/icons/rdf_w3c_icon.128.gif",
    "projectUrl": "http://www.dotnetrdf.org",
    "licenseUrl": "http://www.opensource.org/licenses/MIT",
    "repository": {
      "type": "git",
      "url": "https://github.com/dotnetrdf/dotnetrdf"
    },
    "owners": [ "RobVesse", "kal_ahmed" ]
  },
  "dependencies": {
    "Newtonsoft.Json": "9.0.1",
    "VDS.Common": "1.7.0",
    "StyleCop.Analyzers": {
      "version": "1.0.0",
      "type": "build"
    }
  },
  "tools": {
    "dotnet-version": "1.1.0"
  },

  "frameworks": {
    "netstandard1.4": {
      "dependencies": {
        "NETStandard.Library": "1.6.0",
        "System.ComponentModel.TypeConverter": "4.1.0",
        "System.Globalization.Extensions": "4.0.1",
        "System.Net.Requests": "4.0.11",
        "System.Reflection.TypeExtensions": "4.1.0",
        "System.Threading.Thread": "4.0.0",
        "System.Xml.XmlDocument": "4.0.1",
        "System.Xml.XmlSerializer": "4.0.11"
      },
      "buildOptions": {
        "define": [ "NO_SYSTEMCONFIGURATION", "NO_PROXY", "NO_XSL", "NO_HTMLAGILITYPACK", "NO_DATA", "NO_WEB", "NO_PROCESS", "NO_URICACHE", "NETCORE" ],
        "compile": {
          "exclude": [
            "Web/**/*.cs",
            "Update/Protocol/*.cs",
            "Compatibility/Portable/**/*.cs"
          ]
        }
      }
    },
    "net40": {
      "dependencies": {
        "HtmlAgilityPack": "1.4.9.5"
      },
      "buildOptions": {
        "define": [ "TRACE", "NET40" ],
        "compile": {
          "exclude": [
            "Compatibility/**/*.cs"
          ]
        }
      },
      "frameworkAssemblies": {
        "System.Configuration": "4.0.0.0",
        "System.Data": "4.0.0.0",
        "System.Security": "4.0.0.0",
        "System.Web": "4.0.0.0",
        "System.Xml": "4.0.0.0"
      }
    },
    "net40-client": {
      "dependencies": {
        "HtmlAgilityPack": "1.4.9.5"
      },
      "buildOptions": {
        "define": [ "TRACE", "NO_WEB", "NET40" ],
        "compile": {
          "exclude": [
            "Compatibility/Core/*.cs",
            "Compatibility/Portable/**/*.cs"
          ]
        }
      },
      "frameworkAssemblies": {
        "System.Configuration": "4.0.0.0",
        "System.Data": "4.0.0.0",
        "System.Security": "4.0.0.0",
        "System.Xml": "4.0.0.0"
      }
    },
    "net35": {
      "dependencies": {
        "HtmlAgilityPack": "1.4.9.5"
      },
      "buildOptions": {
        "define": [ "TRACE" ],
        "compile": {
          "exclude": [
            "Compatibility/**/*.cs"
          ]
        }
      },
      "frameworkAssemblies": {
        "System.Configuration": "2.0.0.0",
        "System.Data": "2.0.0.0",
        "System.Security": "2.0.0.0",
        "System.Web": "2.0.0.0",
        "System.Xml": "2.0.0.0"
      }
    },
    "net35-client": {
      "dependencies": {
        "HtmlAgilityPack": "1.4.9.5"
      },
      "buildOptions": {
        "define": [ "TRACE", "NO_WEB" ],
        "compile": {
          "exclude": [
            "Compatibility/Core/*.cs",
            "Compatibility/Portable/**/*.cs"
          ]
        }
      },
      "frameworkAssemblies": {
        "System.Configuration": "2.0.0.0",
        "System.Data": "2.0.0.0",
        "System.Security": "2.0.0.0",
        "System.Xml": "2.0.0.0"
      }
    },
    ".NETPortable,Version=v4.0,Profile=Profile328": {
      "buildOptions": {
        "define": [
          "TRACE",
          "PORTABLE",
          "SILVERLIGHT",
          "WINDOWS_PHONE",
          "NO_WEB",
          "NO_ASP",
          "NO_DATA",
          "NO_COMPRESSION",
          "NO_HTMLAGILITYPACK",
          "NO_XSL",
          "NO_SYSTEMCONFIGURATION",
          "NO_PROXY",
          "NO_XMLDOM",
          "NO_RWLOCK",
          "NO_STOPWATCH",
          "NO_FILE",
          "NO_SYNC_HTTP",
          "NO_PROCESS",
          "NO_NORM",
          "NO_URICACHE"
        ],
        "compile": {
          "exclude": [
            "Compatibility/Core/*.cs"
          ],
          "include": [
            "Compatibility/Portable/**/*.cs"
          ]
        }
      },
      "frameworkAssemblies": {
        "mscorlib": "2.0.5.0",
        "System": "2.0.5.0",
        "System.Net": "2.0.5.0",
        "System.Xml": "2.0.5.0"
      },
      "dependencies": {
        "Portable.Runtime": "1.0.0-test1"
      }
    }
  },

  "buildOptions": {
    "keyFile": "../../Build/dotNetRDF.snk",
    "outputName": "dotNetRDF",
    "embed": [
      "Configuration/configuration.ttl"
    ],
    "xmlDoc": true,
    "additionalArguments": [ "/ruleset:../../dotnetrdf.ruleset", "/additionalfile:../../stylecop.json" ]
  }

}

