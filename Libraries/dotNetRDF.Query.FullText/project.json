{
  "name": "dotNetRDF.Query.FullText",
  "title": "dotNetRDF.Query.FullText",
  "version": "1.0.13-pre6",
  "description": "dotNetRDF.Query.FullText provides Full Text SPARQL support as a plugin for the dotNetRDF Leviathan SPARQL Engine using Lucene.Net",
  "copyright": "Copyright Â© dotNetRDF Project 2009-2017",

  "authors": [ "RobVesse", "tpluscode", "kal_ahmed", "ronmichael" ],
  "packOptions": {
    "iconUrl": "http://www.w3.org/RDF/icons/rdf_w3c_icon.128.gif",
    "projectUrl": "http://www.dotnetrdf.org",
    "licenseUrl": "http://www.opensource.org/licenses/MIT",
    "repository": {
      "type": "git",
      "url": "https://github.com/dotnetrdf/dotnetrdf"
    },
    "owners": [ "RobVesse", "kal_ahmed" ],
    "tags": [ "RDF", "Semantic", "Web", "SPARQL", "Query", "Full", "Text", "Lucene" ]
  },

  "dependencies": {
    "dotNetRDF": "1.0.13-pre6",
    "Lucene.Net": "3.0.3",
    "SharpZipLib": "0.86.0",
    "StyleCop.Analyzers": {
      "version": "1.0.0",
      "type": "build"
    }
  },
  "tools": {
    "dotnet-version": "1.1.0"
  },

  "frameworks": {
    "net40": {
      "buildOptions": {
        "define": [ "TRACE", "NET40" ]
      },
      "frameworkAssemblies": {
        "System.Xml": "4.0.0.0"
      }
    },
    "net40-client": {
      "buildOptions": {
        "define": [ "TRACE" ]
      },
      "frameworkAssemblies": {
        "System.Xml": "4.0.0.0"
      }
    },
    "net35": {
      "buildOptions": {
        "define": [ "TRACE", "NET40" ]
      },
      "frameworkAssemblies": {
        "System.Xml": "2.0.0.0"
      }
    },
    "net35-client": {
      "buildOptions": {
        "define": [ "TRACE" ]
      },
      "frameworkAssemblies": {
        "System.Xml": "2.0.0.0"
      }
    }
  },

  "buildOptions": {
    "keyFile": "../../Build/dotNetRDF.snk",
    "outputName": "dotNetRDF.Query.FullText",
    "embed": [
      "FullText.ttl"
    ],
    "xmlDoc": true,
    "additionalArguments": [ "/ruleset:../../dotnetrdf.ruleset", "/additionalfile:../../stylecop.json" ]
  }

}

