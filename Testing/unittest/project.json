{
  "title": "dotNetRDF.Test",
  "version": "1.0.13-pre6",

  "dependencies": {
    "dotNetRDF.Query.FullText": "1.0.13-pre6",
    "dotNetRDF.Data.Virtuoso": "1.0.13-pre6",
    "NUnit": "3.6.1",
    "Moq": "4.2.1409.1722",
    "dotnet-test-nunit": "3.4.0-*",
    "dotNetRDF": "1.0.13-pre6"
  },
  "tools": {
    "dotnet-version": "1.1.0"
  },
  "testRunner": "nunit",
  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.Runtime.Serialization": "4.0.0.0",
        "System.Data": "4.0.0.0"
      },
      "buildOptions": {
        "define": [ "TRACE", "NET40" ],
        "copyToOutput": [
          "resources/**/*.*",
          "resources/*.*"
        ],
        "embed": [
          "Query/SampleWeightings.n3",
          "embedded.ttl"
        ],
        "compile": {
          "exclude": [
            "Compatibility/*.cs"
          ]
        }
      }
    }
  },

  "buildOptions": {
    "outputName": "dotNetRDF.Test",
    "keyFile": "../../Build/dotNetRDF.snk"
  }
}
