  <target name="compile-data-virtuoso-clientprofile" description="Compiles the dotNetRDF Data.Virtuoso Client Profile Library" unless="${dotnetrdf.nocompile}" depends="compile-core-clientprofile,projectgen-data-virtuoso-clientprofile">
